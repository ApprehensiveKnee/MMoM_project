    region      reg_2 block -30 30 -30 30 ${quote0} ${quote1} units box
    region      reg_2 block -30 30 -30 30 3.35 ${quote1} units box
    region      reg_2 block -30 30 -30 30 3.35 3.45 units box
    group       ${groups} region ${regions}
    group       grp_2 region ${regions}
    group       grp_2 region reg_2
32 atoms in group grp_2
    displace_atoms ${groups} move 0 0 ${quote} units box
