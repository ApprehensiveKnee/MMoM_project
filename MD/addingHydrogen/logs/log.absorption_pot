unfix           mynpt
unfix           mytth1

# 5. Umbrella sampling
fix             mynvt all nvt temp ${tem} ${tem} 100
fix             mynvt all nvt temp 300 ${tem} 100
fix             mynvt all nvt temp 300 300 100
fix             mytth1 C spring/self ${k2}
fix             mytth1 C spring/self 3.5

variable        cpt loop 51
label           loop
variable        zdes equal  -15+0.60*(${cpt}-1)
variable        zdes equal  -15+0.60*(1-1)
variable        zave equal xcm(H2,z)
fix             mytth2 H2 spring tether ${k1} 2.5 2.5 ${zdes} 0
fix             mytth2 H2 spring tether 0.7 2.5 2.5 ${zdes} 0
fix             mytth2 H2 spring tether 0.7 2.5 2.5 -15 0
run             10000
